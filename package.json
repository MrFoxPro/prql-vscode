{
    "name": "prql",
    "displayName": "PRQL",
    "description": "Modern language for transforming data",
    "repository": {
        "url": "https://github.com/aljazerzen/prql-code.git"
    },
    "publisher": "prql",
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "prql",
                "aliases": [
                    "PRQL",
                    "prql"
                ],
                "extensions": [
                    ".prql"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "prql",
                "scopeName": "source.prql",
                "path": "./syntaxes/prql.tmLanguage.json"
            }
        ]
    }
}
